<div class="container-fluid">
    <div class="row">
        <ul class="title list-group col-2 plList">
            <li class="list-group-item d-flex justify-content-between"><span>Playlists</span><span
                    class="addPL">&#10133;</span></li>
            {{#each playlists as |pl|}}
            <li class="list-group-item playlist d-flex justify-content-between" id="{{pl.id}}">
                <span id="{{pl.id}}">{{pl.name}}</span><span class="deletePL">✖️</span>
            </li>
            {{/each}}
        </ul>
        <div class="videowrapper col-7">
            <iframe width="600" height="345" src="https://www.youtube.com/embed/tgbNymZ7vqY" id="youtube">
            </iframe>
        </div>
        <ul class="title list-group col-3" id="resultsList">
            <li class="list-group-item">Results</li>
        </ul>

    </div>
</div>

<script src="/js/homepage.js"></script>
<script src="/js/navbar.js"></script>